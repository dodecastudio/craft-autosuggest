{
  "name": "dodecastudio/craft-autosuggest",
  "description": "A plain text field for Craft CMS that will suggest existing values from other entries saved in the same section.",
  "type": "craft-plugin",
  "version": "1.0.1",
  "license": "proprietary",
  "keywords": [
    "craft",
    "cms",
    "craftcms",
    "craft-plugin",
    "autosuggest",
    "field",
    "fieldtype"
  ],
  "support": {
    "source": "https://github.com/dodecastudio/craft-autosuggest",
    "docs": "https://github.com/dodecastudio/craft-autosuggest/blob/main/README.md",
    "issues": "https://github.com/dodecastudio/craft-autosuggest/issues"
  },
  "authors": [
    {
      "name": "Dodeca Studio",
      "homepage": "https://dodeca.studio"
    }
  ],
  "require": {
    "php": ">=7.4",
    "craftcms/cms": "^3.0|^4.0"
  },
  "autoload": {
    "psr-4": {
      "dodecastudio\\autosuggest\\": "src/"
    }
  },
  "extra": {
    "treeware": {},
    "name": "AutoSuggest",
    "handle": "auto-suggest",
    "developer": "Dodeca Studio",
    "developerUrl": "https://dodeca.studio",
    "documentationUrl": "https://github.com/dodecastudio/craft-autosuggest/blob/main/README.md",
    "changelogUrl": "https://github.com/dodecastudio/craft-autosuggest/blob/main/CHANGELOG.md",
    "class": "dodecastudio\\autosuggest\\AutoSuggest"
  }
}
